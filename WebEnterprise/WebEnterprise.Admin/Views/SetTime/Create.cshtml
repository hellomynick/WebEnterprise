@model WebEnterprise.ViewModels.Catalog.SetTimeSystem.SetTimeSystemCreateRequest

@{
    ViewData["Title"] = "Tạo mới người dùng";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="admin-body">
    <div class="grid wide">
        <div class="row">
            <div class="col l-4 l-o-4">
                <div class="admin-createUser">
                    <form asp-action="Create" enctype="multipart/form-data" method="post">
                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                        <div class="form-group">
                            <label asp-for="StartDay" class="control-label">Start Day</label>
                            <input asp-for="StartDay" class="form-control" type="date" />
                        </div>
                        <div class="form-group">
                            <label asp-for="EndDay" class="control-label">End Day</label>
                            <input asp-for="EndDay" class="form-control" type="date" />
                        </div>
                        <div class="form-group">
                            <input type="submit" value="Create" class="btn btn-primary" />
                        </div>
                    </form>
                </div>
</section>