@model WebEnterprise.ViewModels.Catalog.SetTimeSystem.SetTimeSystemUpdateRequest
<div class="grid wide">
    <div class="row">
        <div class="col l-12">
            <h1 class="admin-title">Update User</h1>
        </div>
    </div>
    <div class="row">
        <div class="col l-4">
            <a class="btn btn-a" asp-action="Index" asp-controller="User">Back to list</a>
        </div>
    </div>
    <div class="row">
        <div class="col l-4 l-o-4">
            <div class="admin-createUser">
                <form asp-action="Edit" enctype="multipart/form-data" method="post">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <input type="hidden" asp-for="Id" />
                    <div class="form-group">
                        <label asp-for="StartDay" class="control-label"></label>
                        <input asp-for="StartDay" type="date" class="form-control" />
                    </div>
                    <div class="form-group">
                        <label asp-for="EndDay" class="control-label"></label>
                        <input asp-for="EndDay" type="date" class="form-control" />
                    </div>
                    <div class="form-group">
                        <input type="submit" value="Update" class="btn btn-primary" />
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>